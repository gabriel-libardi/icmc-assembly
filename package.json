{
  "name": "icmc-assembly",
  "displayName": "ICMC assembly",
  "description": "ICMC assembly language support for Visual Studio Code (with tusca's preprocessor features)",
  "icon": "images/icon.jpg",
  "publisher": "Gabriel Libardi",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "license": "GPL3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabriel-libardi/icmc-assembly"
  },
  "contributes": {
    "languages": [{
      "id": "icmc-assembly",
      "aliases": ["ICMC assembly", "icmc-assembly"],
      "extensions": [".asm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "icmc-assembly",
      "scopeName": "source.ICMCassembly",
      "path": "./syntaxes/icmc-assembly.tmLanguage.json"
    }]
  }
}
