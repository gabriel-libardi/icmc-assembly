{
  "name": "icmc-assembly",
  "displayName": "ICMC assembly",
  "description": "ICMC assembly language support for Visual Studio Code (with tusca's preprocessor features)",
  "version": "0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "icmc-assembly",
      "aliases": ["ICMC assembly", "icmc-assembly"],
      "extensions": [".asm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "icmc-assembly",
      "scopeName": "source.assembly.icmc",
      "path": "./syntaxes/icmc-assembly.tmLanguage.json"
    }]
  }
}
